{
  "name": "@vue-composition-form/resolvers",
  "version": "0.0.1-beta.2",
  "description": "Vue Composition Form validation resolvers",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.mjs",
      "require": "./dist/index.cje.js",
      "umd": "./dist/index.umd.js"
    },
    "./yup": {
      "import": "./yup/dist/index.esm.mjs",
      "require": "./yup/dist/index.cje.js",
      "umd": "./yup/dist/index.umd.js"
    },
    "./zod": {
      "import": "./zod/dist/index.esm.mjs",
      "require": "./zod/dist/index.cje.js",
      "umd": "./zod/dist/index.umd.js"
    }
  },
  "files": [
    "dist",
    "yup/dist",
    "zod/dist"
  ],
  "scripts": {
    "clean": "rimraf dist && rimraf yup/dist && rimraf zod/dist",
    "build": "esno scripts/build.ts",
    "build:rollup": "rollup -c",
    "dev": "pnpm build:rollup -- -w",
    "publish:ci": "esno scripts/publish.ts"
  },
  "license": "MIT",
  "author": "Alex Liu <dsa1314@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mini-ghost/vue-composition-form.git",
    "directory": "packages/resolvers"
  },
  "bugs": {
    "url": "https://github.com/mini-ghost/vue-composition-form/issues"
  },
  "homepage": "https://github.com/mini-ghost/vue-composition-form#readme",
  "keywords": [
    "scheme",
    "validation",
    "scheme-validation",
    "vue-composition-form",
    "yup",
    "zod"
  ],
  "devDependencies": {
    "@vue-composition-form/core": "workspace:*",
    "yup": "^0.32.11",
    "zod": "^3.17.3"
  }
}
